# ZeroTrustGNN API Configuration
# Copy this file to .env and fill in your values

# API Base URL - change this for production deployment
# Local: http://localhost:8000
# Vercel: https://your-backend.vercel.app
VITE_API_BASE_URL=http://localhost:8000

# Your API key from Replit Secrets or Vercel Environment Variables
VITE_API_KEY=your_api_key_here

# Mixpanel Analytics Token (Get from https://mixpanel.com/project/settings)
# Free tier: 1M events/month - leave empty to disable analytics
VITE_MIXPANEL_TOKEN=
